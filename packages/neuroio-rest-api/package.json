{
  "name": "@neuroio/rest-api",
  "version": "2.0.0",
  "description": "Library for work with Neuroio REST API",
  "main": "./dist/index.node.js",
  "types": "./dist/@types/index.d.ts",
  "browser": {
    "./dist/index.node.js": "./dist/index.web.js"
  },
  "scripts": {
    "check-types": "tsc",
    "create-declaration": "tsc --noEmit false --emitDeclarationOnly --declarationDir ./dist/@types --declaration true",
    "build": "NODE_ENV=production webpack",
    "prepublishOnly": "npm run build && npm run create-declaration",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "isomorphic-form-data": "^2.0.0",
    "object-sizeof": "^1.6.0",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/axios": "^0.14.0",
    "@types/isomorphic-form-data": "^2.0.0",
    "@types/jest": "^25.2.1",
    "@types/qs": "^6.9.2",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-prettier": "^3.1.3",
    "file-api": "^0.10.4",
    "jest": "^26.0.1",
    "jest-mock-axios": "^4.0.0",
    "parallel-webpack": "^2.6.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.0",
    "ts-loader": "^7.0.3",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "keywords": [],
  "author": "Dmitriy Balashov <mockedcat@gmail.com>",
