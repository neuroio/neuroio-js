{
  "name": "@neuroio/rest-api",
  "version": "4.2.2",
  "description": "Library for work with Neuroio REST API",
  "main": "./dist/index.node.js",
  "types": "./dist/@types/index.d.ts",
  "browser": {
    "./dist/index.node.js": "./dist/index.web.js"
  },
  "scripts": {
    "check-types": "tsc",
    "create-declaration": "tsc --noEmit false --emitDeclarationOnly --declarationDir ./dist/@types --declaration true",
    "build": "NODE_ENV=production webpack",
    "prepublishOnly": "npm run build && npm run create-declaration",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "isomorphic-form-data": "^2.0.0",
    "object-sizeof": "^1.6.1",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/axios": "^0.14.0",
    "@types/isomorphic-form-data": "^2.0.0",
    "@types/jest": "^26.0.24",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.4.1",
    "file-api": "^0.10.4",
    "jest": "^26.6.3",
    "jest-mock-axios": "^4.4.1",
    "parallel-webpack": "^2.6.0",
    "prettier": "^2.4.1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "typescript": "^3.9.10",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "keywords": [],
