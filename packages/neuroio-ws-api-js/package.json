{
  "name": "@neuroio/websocket",
  "version": "1.0.0-beta.2",
  "description": "Library for work with Neuroio WebSocket API",
  "main": "./dist/index.node.js",
  "browser": {
    "./dist/index.node.js": "./dist/index.web.js"
  },
  "types": "./dist/@types/index.d.ts",
  "scripts": {
    "check-types": "tsc",
    "create-declaration": "tsc --noEmit false --emitDeclarationOnly --declarationDir ./dist/@types --declaration true",
    "build": "NODE_ENV=production webpack",
    "prepublishOnly": "npm run build && npm run create-declaration",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "websocket",
    "API"
  ],
  "author": "Dmitriy Balashov <mockedcat@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "isomorphic-ws": "^4.0.1",
    "lodash.debounce": "^4.0.8",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^12.12.38",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "mock-socket": "^9.0.3",
    "parallel-webpack": "^2.6.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1",
    "ts-jest": "^25.5.0",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
